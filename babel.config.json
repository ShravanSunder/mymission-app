{
   "presets": ["@babel/preset-env", ["@babel/preset-react", { "runtime": "automatic", "importSource": "@emotion/react" }]],
   "plugins": [
      "babel-plugin-macros",
      "@emotion/babel-plugin",
      [
         "formatjs",
         {
            "idInterpolationPattern": "[sha512:contenthash:base64:6]",
            "ast": true
         }
      ]
   ]
}
