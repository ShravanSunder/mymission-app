   "scripts": {
      "start": "cross-env NODE_ENV=development  NODE_OPTIONS=--max-old-space-size=4096  webpack serve --progress --config build-utils/webpack.config.js --env env=dev",
      "build:prod": "cross-env NODE_ENV=production webpack --config build-utils/webpack.config.js --env env=prod",
      "build:workbox-simple": "workbox injectManifest workbox-config.js",
      "build:workbox-esbuild": "npx esbuild src/serviceworker/serviceworker.ts --bundle --define:process.env.NODE_ENV=\"production\" --outfile='/dist/sw.generated.js' && npx workbox-cli injectManifest workbox.config.js",
      "build:tailwind": "postcss src/tailwind.css -o src/tailwind.generated.css",
      "watch:tailwind": "postcss -w src/tailwind.css -o src/tailwind.generated.css",
      "build:analyze": "yarn run build:prod --env addon=bundleanalyze",
      "test": "jest --runInBand",
      "test:update": "jest -u",
      "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
      "lint": "eslint './src/**/*.{js,ts,tsx,jsx}' --fix",
      "lint:quiet": "eslint 'src/**/.{js,ts,tsx,jsx}' --quiet --fix",
      "lint:commit": "yarn run pretty-quick --staged --verbose --pattern \"./src/**/*.(js|jsx|ts|tsx)\"",
      "storybook": "start-storybook -p 6006",
      "build:storybook": "build-storybook"
   },
