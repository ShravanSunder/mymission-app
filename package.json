      "build:workbox-simple": "workbox injectManifest workbox-config.js",
      "build:analyze": "yarn run build:prod --env addon=bundleanalyze",
      "test": "jest --runInBand",
      "test:update": "jest -u",
      "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
      "lint": "eslint 'src/*.{js,ts,tsx}' --fix",
      "lint:quiet": "eslint 'src/*.{js,ts,tsx}' --quiet --fix",
      "commit": "yarn run pretty-quick --staged --verbose --pattern \"./src/*.*(js|jsx|ts|tsx)\"",
      "build:tailwind": "postcss src/tailwind.css -o src/tailwind.generated.css",
      "watch:tailwind": "postcss -w src/tailwind.css -o src/tailwind.generated.css",
      "storybook": "start-storybook -p 6006",
      "build:storybook": "build-storybook"
